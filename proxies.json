{
	"$schema": "http://json.schemastore.org/proxies",
	"proxies": {
		"slack-events": {
			"matchCondition": {
				"methods": ["POST"],
				"route": "/slack/events"
			},
			"backendUri": "https://localhost/api/slack/events"
		},
		"slack-interactions": {
			"matchCondition": {
				"methods": ["POST"],
				"route": "/slack/interactions"
			},
			"backendUri": "https://localhost/api/slack/interactions"
		}
	}
}
